{
  "name": "graph-validate-lib",
  "version": "1.0.2",
  "description": "图形验证库",
  "main": "lib/lib.umd.js",
  "module": "lib/lib.module.js",
  "scripts": {
    "watch": "rollup -w -c --environment FORMAT:umd",
    "build:umd": "rollup -c --environment FORMAT:umd",
    "build:min": "rollup -c --environment FORMAT:min",
    "build:esm": "rollup -c --environment FORMAT:esm",
    "build": "rm -rf lib/* && run-p build:umd build:min build:esm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/g0ngjie/graph-validate-lib.git"
  },
  "keywords": [
    "graph-validate-lib",
    "validate",
    "graph-validate",
    "validate-lib"
  ],
  "author": "Gj <gongjie0422@163.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/g0ngjie/graph-validate-lib/issues"
  },
  "homepage": "https://github.com/g0ngjie/graph-validate-lib#readme",
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "lodash.merge": "^4.6.2",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.70.2",
    "rollup-plugin-uglify": "^6.0.4",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "rollup-plugin-typescript2": "^0.31.2"
  }
}
